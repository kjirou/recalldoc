{
  "manifest_version": 3,
  "name": "Recalldoc",
  "version": "1.0.0",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.esa.io/*", "https://*.kibe.la/*"],
      "js": ["dist/webpacked.js"]
    }
  ],
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128-original.png"
  }
}
